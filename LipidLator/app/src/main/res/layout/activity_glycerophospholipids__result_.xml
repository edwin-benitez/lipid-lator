<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.onerom.lipidlator.Glycerophospholipids_Result_Activity">


    <Button
        android:id="@+id/button2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="8dp"
        android:layout_marginEnd="8dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        android:background="@android:color/black"
        android:text="@string/Home"
        android:textColor="@android:color/background_light"
        android:textSize="18sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/formula_Glycerophospholipids_input"
        app:layout_constraintVertical_bias="0.513"
        tools:layout_editor_absoluteX="235dp"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/tvGlycerophospholipids"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/glycerophospholipids"
        android:textColor="@android:color/black"
        android:textSize="24sp"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginTop="16dp"
        android:layout_marginRight="7dp"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginLeft="8dp"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginStart="8dp"
        android:layout_marginEnd="7dp" />

    <TextView
        android:id="@+id/tvGlycerophospholipidsIon"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/ion"
        android:textColor="@android:color/black"
        android:textSize="18sp"
        android:layout_marginLeft="8dp"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginStart="8dp"
        tools:layout_editor_absoluteY="60dp"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/tvGlyHeadGroup"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="16dp"
        android:layout_marginStart="8dp"
        android:text="@string/head_group"
        android:entries="@array/gly_headgroup_array_values"
        android:textColor="@android:color/black"
        android:textSize="18sp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toLeftOf="@+id/spnGlyHeadGroup"
        android:layout_marginEnd="8dp"
        android:layout_marginRight="8dp"
        app:layout_constraintHorizontal_bias="0.0"
        android:layout_marginTop="25dp"
        app:layout_constraintTop_toBottomOf="@+id/tvGlycerophospholipidsIon" />

    <TextView
        android:id="@+id/tvSn1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="-5dp"
        android:layout_marginStart="8dp"
        android:text="@string/sn1_1"
        android:textColor="@android:color/black"
        android:textSize="18sp"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginTop="23dp"
        app:layout_constraintTop_toBottomOf="@+id/tvGlyHeadGroup" />

    <TextView
        android:id="@+id/tvSn2_1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:text="@string/sn2_1"
        android:textColor="@android:color/black"
        android:textSize="18sp"
        android:layout_marginTop="14dp"
        app:layout_constraintTop_toBottomOf="@+id/tvSn1"
        android:layout_marginLeft="8dp"
        app:layout_constraintLeft_toLeftOf="parent" />

    <Button
        android:id="@+id/btnGly"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="16dp"
        android:layout_marginEnd="8dp"
        android:layout_marginLeft="16dp"
        android:layout_marginStart="8dp"
        android:background="@android:color/black"
        android:text="@string/btnGo_back"
        android:textColor="@android:color/background_light"
        android:textSize="18sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toLeftOf="@+id/button2"
        android:layout_marginRight="8dp"
        android:layout_marginTop="8dp"
        app:layout_constraintTop_toBottomOf="@+id/tvFormula"
        app:layout_constraintHorizontal_bias="0.357"
        app:layout_constraintVertical_bias="0.75" />

    <Spinner
        android:id="@+id/spnGlyIon"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginRight="8dp"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginStart="8dp"
        android:layout_marginEnd="8dp"
        app:layout_constraintLeft_toRightOf="@+id/tvGlycerophospholipidsIon"
        android:layout_marginLeft="0dp"
        app:layout_constraintHorizontal_bias="1.0"
        android:layout_marginTop="15dp"
        app:layout_constraintTop_toBottomOf="@+id/tvGlycerophospholipids" />

    <Spinner
        android:id="@+id/spnSn1_1"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginRight="8dp"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginStart="8dp"
        android:layout_marginEnd="8dp"
        app:layout_constraintLeft_toRightOf="@+id/tvSn1"
        android:layout_marginLeft="8dp"
        app:layout_constraintHorizontal_bias="1.0"
        android:layout_marginTop="17dp"
        app:layout_constraintTop_toBottomOf="@+id/spnGlyHeadGroup" />

    <Spinner
        android:id="@+id/spnSn2_1"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginRight="8dp"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginStart="8dp"
        android:layout_marginEnd="8dp"
        app:layout_constraintLeft_toRightOf="@+id/tvSn2_1"
        android:layout_marginLeft="8dp"
        app:layout_constraintHorizontal_bias="0.0"
        android:layout_marginTop="10dp"
        app:layout_constraintTop_toBottomOf="@+id/spnSn1_1" />

    <Spinner
        android:id="@+id/spnGlyHeadGroup"
        android:layout_width="237dp"
        android:layout_height="29dp"
        android:layout_marginEnd="8dp"
        android:layout_marginRight="16dp"
        android:layout_marginStart="8dp"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginTop="23dp"
        app:layout_constraintTop_toBottomOf="@+id/spnGlyIon" />

    <TextView
        android:id="@+id/M_Z_Ratio"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/m_z_ratio_chE"
        android:textColor="@color/black"
        android:textSize="24sp"
        tools:ignore="RtlHardcoded"
        app:layout_constraintRight_toLeftOf="@+id/M_Z_Ratio_Input"
        android:layout_marginRight="40dp"
        android:layout_marginLeft="8dp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        android:layout_marginTop="36dp"
        app:layout_constraintTop_toBottomOf="@+id/tvSn2_1" />

    <EditText
        android:id="@+id/M_Z_Ratio_Input"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:ems="10"
        android:inputType="numberDecimal"
        tools:layout_editor_absoluteX="153dp"
        tools:ignore="MissingConstraints"
        android:layout_marginTop="23dp"
        app:layout_constraintTop_toBottomOf="@+id/spnSn2_1" />

    <TextView
        android:id="@+id/Abbreviation_Glycerophospholipids"
        android:layout_width="176dp"
        android:layout_height="30dp"
        android:text="@string/abbreviation"
        android:textSize="24sp"
        android:textColor="@color/black"
        android:layout_marginLeft="8dp"
        app:layout_constraintLeft_toLeftOf="parent"
        tools:ignore="RtlHardcoded"
        android:layout_marginTop="29dp"
        app:layout_constraintTop_toBottomOf="@+id/M_Z_Ratio" />

    <EditText
        android:id="@+id/abbreviation_Glycerophospholipids_input"
        android:layout_width="187dp"
        android:layout_height="42dp"
        android:ems="10"
        android:inputType="numberDecimal"
        tools:layout_editor_absoluteX="181dp"
        tools:ignore="MissingConstraints"
        android:layout_marginTop="25dp"
        app:layout_constraintTop_toBottomOf="@+id/M_Z_Ratio_Input" />

    <EditText
        android:id="@+id/formula_Glycerophospholipids_input"
        android:layout_width="187dp"
        android:layout_height="42dp"
        android:ems="10"
        android:inputType="numberDecimal"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="181dp"
        android:layout_marginTop="25dp"
        app:layout_constraintTop_toBottomOf="@+id/abbreviation_Glycerophospholipids_input" />

    <TextView
        android:id="@+id/tvFormula"
        android:layout_width="176dp"
        android:layout_height="30dp"
        android:layout_marginLeft="0dp"
        android:layout_marginTop="96dp"
        android:text="@string/formula_Glycerophospholipids_result"
        android:textColor="@color/black"
        android:textSize="24sp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/M_Z_Ratio"
        android:layout_marginStart="-2dp"
        app:layout_constraintRight_toLeftOf="@+id/formula_Glycerophospholipids_input"
        android:layout_marginEnd="8dp"
        android:layout_marginRight="8dp"
        app:layout_constraintHorizontal_bias="1.0" />

</android.support.constraint.ConstraintLayout>
