<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.onerom.lipidlator.WaxEstersActivity">

    <TextView
        android:id="@+id/WaxEstersTV"
        android:layout_width="147dp"
        android:layout_height="31dp"
        android:text="@string/waxEstersTitle"
        android:textColor="@color/black"
        tools:textAlignment="center"
        tools:textSize="24sp"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginTop="26dp"
        android:layout_marginRight="8dp"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginLeft="8dp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintHorizontal_bias="0.502" />

    <TextView
        android:id="@+id/WaxEstersIonTV"
        android:layout_width="45dp"
        android:layout_height="25dp"
        android:layout_marginLeft="25dp"
        android:layout_marginTop="19dp"
        android:text="@string/ionTV"
        android:textAppearance="@style/TextAppearance.AppCompat"
        android:textColor="@color/black"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/WaxEstersTV"
        tools:textSize="16sp"
        tools:ignore="RtlHardcoded" />
</android.support.constraint.ConstraintLayout>
